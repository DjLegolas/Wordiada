<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>

<HBox fx:id="userInfoHBox" alignment="CENTER" spacing="3.0" xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1" fx:controller="desktopUI.userInfo.UserInfoController">
  <Label fx:id="nameLabel" text="name" />
  <Label text="ID:" />
  <Label fx:id="idLabel" text="ID#" />
  <Label fx:id="playerTypeLabel" text="type" />
  <Label text="Score:" />
  <Label fx:id="scoreLabel" text="score#" />
  <Button fx:id="detailsButton" disable="true" mnemonicParsing="false" text="Details" onAction="#showWords" />
</HBox>
